Crear una calculadora en Python

import match

# Parte 1: Operaciones matemáticas
def suma(a, b):
    return a + b

def resta(a, b):
    return a - b

def multiplicacion(a, b):
    return a * b

def division(a, b):
    if b != 0:
        return a / b
    else:
        return "Error: No se puede dividir entre cero."

def raiz_cuadrada(a):
    return math.sqrt(a)

def elevar_potencia(a, b):
    return a ** b

def obtener_numeros():

    num1 = float(input("Ingrese el primer número: "))
    num2 = float(input("Ingrese el segundo número: "))
    
return num1, num2
